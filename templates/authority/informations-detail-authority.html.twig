{% if sameInstance(notice,'App\\Model\\Authority') and (notice.formAdopted is not empty or notice.type is not empty or notice.name is not empty) %}
    <li class="list-information__item">
        {% if notice.type is not empty %}
            <span class="list-information__label">{{ ("Type")|trans }}</span>
        {% endif %}
        <ul class="list-information__sub-list" lang="fr">
            <li class="list-information__sub-item">
                {{ notice.type }}
            </li>
            {% if notice.formAdopted is not empty %}
                <li class="list-information__sub-item">
                    {{ "bloc.autority-result-information.authority-type"|trans }}{{ notice.formAdopted }}
                </li>
            {% endif %}
            {% if notice.name is not empty %}
                <li class="list-information__sub-item">
                    {{ notice.name }}
                </li>
            {% endif %}
        </ul>
    </li>
{% endif %}

