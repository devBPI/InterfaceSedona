<div class="dropdown mb-3" aria-label="{{ 'bloc.relauch-search.title-aria'|trans }}" lang="fr">
	<span class="sr-only">{{ 'bloc.relauch-search.title-sr-only'|trans }}</span>
	<button type="button" id="dropdown-recherche" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="listbox" aria-expanded="false">
		{{ 'bloc.relauch-search.button'|trans }}
	</button>
	<div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdown-recherche" role="listbox" tabindex="-1">
		{% for image, element in {
			'logo-biblio-paris.svg': {name: 'Les bibliothèques de la ville de Paris', href: 'https://bibliotheques.paris.fr/Default/search.aspx?SC=CATALOGUE&QUERY=' ~ keyword },
			'logo-sudoc.svg': {name:'Les bibliothèques universitaires', href:'http://www.sudoc.abes.fr//DB=2.1/CMD?ACT=SRCHA&IKT=1016&SRT=RLV&TRM=' ~ keyword },
			'logo-bnf.svg': {name:'La BnF', href:'http://catalogue.bnf.fr/rechercher.do?motRecherche=' ~ keyword },
			'logo-wikipedia.svg': {name:'L\'encyclopédie Wikipédia', href:'http://fr.wikipedia.org/wiki/Special:Recherche?search=' ~ keyword },
			'logo-universalis.svg': {name:'L\'Encyclopædia Universalis', href:'https://www.universalis.fr/recherche/q/' ~ keyword },
			'logo-biblio-film.svg': {name:'La bibliothèque du film', href:'http://www.cineressources.net/recherche_t.php?textfield=' ~ keyword ~ '&rech_type=E&rech_mode=contient' },
			'logo-philharmonie.svg': {name:'La médiathèque de la Philharmonie de Paris', href:'http://catalogue.philharmoniedeparis.fr/search.aspx?SC=CATALOGUE&QUERY=' ~ keyword }
		} %}
			<a href="{{ element.href }}" target="_blank" lang="fr" role="option"
			   class="js-search-patner dropdown-item gtm-search-partner"
				title="{{ element.name }} - {{ 'global.new-windows'|trans }}">
				{% if image is not empty %}
					<img src="{{ asset('images/logos/'~image) }}" alt="" class="dropdown-image-start" />
				{% endif %}
				{{ element.name }}
			</a>
		{% endfor %}
	</div>
</div>
