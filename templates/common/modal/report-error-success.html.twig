<div data-toggle-refresh="{{ path('common-report-error',{'permalink': permalink}) }}">
	<div class="modal-header">
		<h1 class="n1 modal-header__title" id="modal-report-title">{{ 'modal.report.title'|trans }}</h1>
		<button class="close" type="button" data-dismiss="modal">
			<img src="{{ asset('images/icones/icone-croix-blanc.svg') }}" alt="{{ 'global.modal-close'|trans }}" />
		</button>
	</div>

	<div class="modal-body">
		{{ 'modal.body.report-error'|trans }}
	</div>

	<div class="modal-footer">
		<button type="button" data-dismiss="modal" class="btn btn-lg btn-secondary">
			{{ 'global.modal-btn-close'|trans }}</button>
	</div>
</div>
