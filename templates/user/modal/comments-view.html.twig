<div role="dialog" aria-modal="true" aria-labelledby="modal-comments-view-title" class="modal fade" id="modal-comments-view" tabindex="-1" lang="fr">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content">
			<div class="modal-header">
				<h1 class="n1 modal-header__title" id="modal-comments-view-title">{{ 'modal.comments-view.title'|trans }}</h1>
				<button class="close" type="button" data-dismiss="modal">
					<img src="{{ asset('images/icones/icone-croix-blanc.svg') }}" alt="{{ 'global.modal-close'|trans }}" />
				</button>
			</div>

			<div class="modal-body">
				<p>Pas de commentaire</p>
			</div>

			<div class="modal-footer">
				<button type="button" data-dismiss="modal" class="btn btn-lg btn-secondary">{{ 'modal.comments-view.close'|trans }}</button>
			</div>
		</div>
	</div>
</div>
